#include <stdio.h>
 
main(){
int x, y, z;
int N[1000];
 
 
scanf("%d", &z); // para dizer ate quantos o numero pode ir dentro do vetor
 
 
x=0;
 
for(y = 0; y < 1000; y++){ //para amarzenar os valores do vetor
    printf("N[%d] = %d\n", y, x); //apresentar os valores
    x++;
    if(x == z)x = 0;
}
 
return 0;
 
}
