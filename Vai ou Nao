#include<stdio.h>
#include<stdlib.h>
 
main () { 
 
    int n;
     
     
    while(scanf("%d",&n) !=EOF ){ // While junto com o EOF para nao deixa o programa para de funcionar
         
        if(n==0) // n=0 para fazer o teste se vai ter copa ou nao.
        printf("vai ter copa!\n");
        else
        printf("vai ter duas!\n");
    }
     
     
    return 0;
 
}
